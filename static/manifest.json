{
  "manifest_version": 3,
  "default_locale": "en",
  "name": "__MSG_ext_name__",
  "description": "__MSG_ext_desc__",
  "version": "7.2.0",
  "minimum_chrome_version": "105",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "action": {
    "default_title": "__MSG_default_title__"
  },
  "permissions": [
    "scripting",
    "background",
    "bookmarks",
    "storage",
    "unlimitedStorage",
    "tabs",
    "notifications",
    "contextMenus"
  ],
  "optional_permissions": [
    "clipboardRead",
    "search"
  ],
  "optional_host_permissions": [
    "https://google.com/*",
    "https://www.bing.com/*",
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
